{
  "name": "kriger-campus-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "ng run kriger-campus-website:server && npm run webpack:ssr",
    "webpack:ssr": "webpack --config webpack.server.config.js",
    "serve:ssr": "node dist/kriger-campus-website-webpack/server.js",
    "build": "npm run empty:dist && ng build --prod  && npm run build:ssr",
    "build:web": "npm run empty:dist && ng build --prod",
    "remove:index": "rm ./dist/kriger-campus-website/index.html",
    "empty:dist": "rm -rf ./dist/",
    "empty:fdist": "rm -rf ./functions/dist/",
    "create:fdist": "mkdir ./functions/dist",
    "build:functions": "npm run --prefix functions build",
    "deploy:web": "firebase deploy --only hosting:prod",
    "deploy:universal": "firebase deploy --only functions:universal",
    "deploy": "npm run deploy:web && npm run deploy:universal"
  },
  "private": true,
